@model Dictionary<Bros.DataModel.User, int>

@{
    ViewBag.Title = "ViewBros";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
    
}

<div class="ViewBros">
    @if (Model.Count() != 0)
    {        
        foreach(Bros.DataModel.User user in Model){
                Html.RenderPartial("_UserPartialView", user);   

        }
    }
    else
    {
        <div class="NoUsers">No users were found.</div>
    }
</div>
