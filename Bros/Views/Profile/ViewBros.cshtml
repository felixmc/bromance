@model IEnumerable<Bros.DataModel.User>

@{
    ViewBag.Title = "ViewBros";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

<div class="ViewBros">
    @if (Model.Count() != 0)
    {
        foreach(Bros.DataModel.User u in Model){
            Html.RenderPartial("_UserPartialView", u);
        }
    }
    else
    {
        <div class="NoUsers">No users were found.</div>
    }
</div>
