@model IEnumerable<Bros.DataModel.BroRequest>

@{
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

<div class="ViewBroRequests">
    @if (Model.Count() != 0)
    {
        foreach(Bros.DataModel.BroRequest b in Model){
            Html.RenderPartial("_BroRequestPartial", b);   
        }
    }
    else
    {
        <div class="NoRequests">Doesn't look like you have any Bro Requests at the moment.</div>
    }
</div>