@model List<Bros.DataModel.User>

<h2 class="title">Browse Bros</h2>

@if (Model.Count() != 0)
{
	foreach (Bros.DataModel.User user in Model)
	{
	<p>
		<div class="mini-profile">@Html.Partial("Partials/_User", user)</div>
		<span class="small">@Html.Partial("Partials/_BroRequestButton", user)
		@Html.Partial("Partials/_BlockBroButton", user)</span>
	</p>
	}
}
else
{
	<p class="error">No users were found.</p>
}