@*@model Dictionary<Bros.DataModel.User, int>*@
@model List<Bros.DataModel.User>

<div class="ViewBros">
    @if (Model.Count() != 0)
    {        
        foreach(Bros.DataModel.User user in Model){
                Html.RenderPartial("Partials/_User", user);   

        }
    }
    else
    {
        <div class="error">No users were found.</div>
    }
</div>
