@model Bros.DataModel.ShoppingCart
@using Bros.DataModel;
@{
    ViewBag.Title = "ViewCart";
}


@if (ViewBag.ItemsInShoppingCart != null)
{

    foreach (ProductQuantity p in ViewBag.ItemsInShoppingCart)
{

    Html.RenderPartial("_JustForCartPartialViewProduct", p);
        
        
}
@Html.ActionLink("Checkout","Checkout")
@Html.ActionLink("Back to Store", "StoreIndex", "Store", null, new { @class = "icon-grocery-store", title = "Store" })
@Html.ActionLink("Home Feed ", "Index", "Home", null, new { @class = "icon-home", title = "Home" })
}